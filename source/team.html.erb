<div class="text-center">
  <h1>Our team</h1>
</div>


<ul class="list-inline text-center">
  <% ["anne", "seb", "romain"].each do |member| %>
    <li class="team_member">
      <h3><%= member.capitalize %></h3>
      <%= image_tag "#{member}.png", :width => '70', class: "img-circle" %>
    </li>
  <% end %>
</ul>

<h3 class="padded text-center">
  <a class="email_link" href="mailto:contact@airbnb.com">
    <i class="fa fa-envelope"></i>contact@airbnb.com
  </a>
</h3>
